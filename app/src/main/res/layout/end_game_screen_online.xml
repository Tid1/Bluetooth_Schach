<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/endGameTextOnline"
        android:layout_width="280dp"
        android:layout_height="339dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="225dp"
        android:layout_marginEnd="66dp"
        android:layout_marginBottom="30dp"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/onlineReturnToMain"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/onlineReturnToMain"
        android:layout_width="135dp"
        android:layout_height="84dp"
        android:layout_marginStart="138dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="138dp"
        android:layout_marginBottom="141dp"
        android:text="@string/return_to_main"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/endGameTextOnline" />
</androidx.constraintlayout.widget.ConstraintLayout>